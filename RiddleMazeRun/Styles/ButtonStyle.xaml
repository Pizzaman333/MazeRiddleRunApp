<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="St-Button" TargetType="Button">
        <Setter Property="Background" Value="{ThemeResource HighlightColorBrush}" />
        <Setter Property="Foreground" Value="{ThemeResource TextColorBrush}" />
    </Style>
    <SolidColorBrush x:Key="HighlightColorBrush" Color="#da291c" />
</ResourceDictionary>
